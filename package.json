{
  "name": "ryancollins.io",
  "version": "1.0.0",
  "description": "Professional Portfolio for Ryan Collins.",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "babel-plugin-webpack-loaders", {
              "config": "./webpack.test.config.js",
              "verbose": false
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testPathDirs": [
      "<rootDir>/app/__tests__"
    ],
    "testPathIgnorePatterns": [
      "/__mocks__/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "<rootDir>/app/__tests__/mocks/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/app/__tests__/mocks/fileMock.js"
    }
  },
  "scripts": {
    "t": "npm run test",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test node --harmony_proxies node_modules/.bin/jest",
    "test:watch": "npm run test -- --watch",
    "build": "`npm bin`/webpack",
    "dev": "`npm bin`/webpack-dev-server",
    "lint": "`npm bin`/eslint . --ext .js --ext .jsx",
    "backend": "node keystone.js",
    "deploy": "NODE_ENV=production `npm bin`/webpack -p",
    "start": "npm run dev && npm run backend",
    "clean": "rm -rf app/dist app/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanCCollins/ryancollins.io.git"
  },
  "keywords": [
    "redux",
    "react",
    "webpack",
    "mocha",
    "sass"
  ],
  "engines": {
    "node": "5.2.0",
    "npm": "3.10.7"
  },
  "author": "Ryan Collins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanCCollins/ryancollins.io/issues"
  },
  "homepage": "https://github.com/RyanCCollins/ryancollins.io#readme",
  "dependencies": {
    "animatewithsass": "^3.2.1",
    "axios": "^0.12.0",
    "babel-core": "^6.3.15",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bookshelf": "^0.9.1",
    "css-loader": "^0.23.0",
    "foundation-sites": "^6.2.3",
    "history": "^1.14.0",
    "html-webpack-plugin": "^2.7.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^15.1.1",
    "json-loader": "^0.5.4",
    "keystone": "https://github.com/keystonejs/keystone.git",
    "knex": "^0.9.0",
    "lru-memoize": "^1.0.1",
    "marked": "^0.3.5",
    "model-transform": "^1.1.0",
    "moment": "^2.13.0",
    "motion-ui": "^1.2.2",
    "node-sass": "^3.4.2",
    "paginator": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.0.2",
    "react-disqus-thread": "^0.4.0",
    "react-dom": "^15.0.2",
    "react-engine": "^3.4.1",
    "react-foundation": "^0.6.8",
    "react-headroom": "^2.1.0",
    "react-icons": "^2.1.0",
    "react-markdown": "^2.3.0",
    "react-motion": "^0.4.4",
    "react-motion-loop": "^1.0.0",
    "react-motion-ui-pack": "^0.7.3",
    "react-photo-gallery": "^4.2.8",
    "react-redux": "^4.4.5",
    "react-redux-toastr": "^3.1.4",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.5",
    "react-scroll": "^1.0.17",
    "react-scrollspy": "^2.0.0",
    "react-shuffle": "^1.0.0",
    "react-slick": "^0.12.2",
    "react-spinkit": "^1.1.7",
    "react-timeout": "^1.0.0",
    "react-tooltip": "^3.0.8",
    "react-waypoint": "^2.0.3",
    "redux": ">=3.1.0",
    "redux-form": "^5.2.5",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^3.0.2",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "svg-react-loader": "^0.3.5",
    "underscore": "^1.8.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta4",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.20.2",
    "file-loader": "^0.8.5",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^7.2.0",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.14.0"
  }
}
